<script>
	BX.Tasks.Component.IframePopup.create({'pathToTasks':'/company/personal/user/1/tasks/task/#action#/#task_id#/','callbacks':{'ADD':'#SHOW_ADDED_TASK_DETAIL#','UPDATE':false,'DELETE':false}});
